// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  prints    Print[]   @relation("Print_user")
  code      Code?     @relation("Code_user")
  tag       Tag?      @relation("Tag_user")
  createdAt DateTime? @default(now())
}

model Print {
  id     String  @id @default(cuid())
  code   String  @unique @default("")
  user   User?   @relation("Print_user", fields: [userId], references: [id])
  userId String? @map("user")

  @@index([userId])
}

model Code {
  id     String  @id @default(cuid())
  code   String  @unique @default("")
  user   User?   @relation("Code_user", fields: [userId], references: [id])
  userId String? @unique @map("user")
}

model Tag {
  id     String  @id @default(cuid())
  code   String  @unique @default("")
  user   User?   @relation("Tag_user", fields: [userId], references: [id])
  userId String? @unique @map("user")
}
